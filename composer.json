{
    "name": "cmercado93/everlytic-api",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Carlos Mercado",
            "email": "cmercado93@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cmercado93\\EverlyticApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cmercado93\\EverlyticApi\\Tests\\": "tests/"
        },
        "files": [
            "tests/Helpers/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cmercado93\\EverlyticApi\\LaravelProviders\\EverlyticApiServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vlucas/phpdotenv": "^5.5@dev"
    }
}
